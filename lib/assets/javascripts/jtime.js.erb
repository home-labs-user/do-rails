(function () {

    // verifica se window jรก mapeia requireJS
    if (!window["requireJS"]) {
        var
            script = document.createElement("script")
            tag = <%= "\'#{javascript_include_tag('require_js')}\'" %>;

        script.src = tag
            .match(/(src=[\"\'])([\/\w])+(\.js)?/gi)[0]
                .replace(/(src=)|([\"\']*)/g, "");

        document.head.appendChild(script);
    }

})();

window.onload = function () {
    requireJS.loadPath("jtime");
    require("jtime");
}
